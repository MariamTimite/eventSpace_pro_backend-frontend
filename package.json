{
  "name": "eventspace-pro",
  "version": "1.0.0",
  "description": "Application de réservation d'espaces événementiels",
  "main": "index.js",
  "scripts": {
    "setup": "node setup.js",
    "test-mongo": "node test-mongodb.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "eventspace",
    "booking",
    "mern",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Mariam TIMITE",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  }
}
